<dialog>
  <tab name="Input Files and Directories">
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Dir">
      <label>Input directory</label>
      <help_brief>Path to input files. {cb;GUI;/data/sims/stm/AgDegNormGravMixSubPW}</help_brief>
      <default>/data/sims/stm/AgDegNormGravMixSubPW</default>
      <type>String</type>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/SitePrefix">
      <label>Site prefix</label>
      <help_brief>Site prefix for input/output files.</help_brief>
      <default>AgDegNormGravMixSubPW</default>
      <type>String</type>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/CasePrefix">
      <label>Case prefix</label>
      <help_brief>Case prefix for input/output files.</help_brief>
      <default>test</default>
      <type>String</type>
    </entry>
  </tab>
  <tab name="Run Options">
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/ChezyOrManning">
      <label>Chezy Or Manning, Chezy-1 or Manning-2</label>
      <help_brief>Chezy Or Manning, Chezy-1 or Manning-2</help_brief>
      <default>1</default>
      <type>Int</type>
      <range>
        <min>1</min>
        <max>2</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/bedloadrelation">
      <label>BedloadRelation, Parker or Wilock, Parker-1 or Wilock-2</label>
      <help_brief>BedloadRelation, Parker or Wilock, Parker-1 or Wilock-2</help_brief>
      <default>1</default>
      <type>Int</type>
      <range>
        <min>1</min>
        <max>2</max>
      </range>
    </entry>
  </tab>
  <tab name="Run Parameters">
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/q">
      <label>Flood discharge [m^2/s]</label>
      <help_brief>Flood discharge [m^2/s]</help_brief>
      <default>6</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/T">
      <label>gravel input [m^2/s]</label>
      <help_brief>gravel input [m^2/s]</help_brief>
      <default>0.004</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/I">
      <label>Intermittency</label>
      <help_brief>Intermittency</help_brief>
      <default>.03</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/e">
      <label>base level [m]</label>
      <help_brief>base level [m]</help_brief>
      <default>0.</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/S">
      <label>initial bed slope</label>
      <help_brief>initial bed slope</help_brief>
      <default>0.005</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/L">
      <label>reach length [m]</label>
      <help_brief>reach length [m]</help_brief>
      <default>50000</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/t">
      <label>Time step [days]</label>
      <help_brief>Time step [days]</help_brief>
      <default>292.2</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/M">
      <label>no. of intervals (100 or less)</label>
      <help_brief>no. of intervals (100 or less)</help_brief>
      <default>25</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/p">
      <label>Number of printouts</label>
      <help_brief>Number of printouts</help_brief>
      <default>6</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/i">
      <label>Iterations per each printout</label>
      <help_brief>Iterations per each printout</help_brief>
      <default>20000</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/k">
      <label>factor by which Ds90 is multiplied for roughness height</label>
      <help_brief>factor by which Ds90 is multiplied for roughness height</help_brief>
      <default>2</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/n">
      <label>factor by which Ds90 is multiplied for active layer thickness</label>
      <help_brief>factor by which Ds90 is multiplied for active layer thickness</help_brief>
      <default>2</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/r">
      <label>Manning-Strickler coefficient, r [-]</label>
      <help_brief>Manning-Strickler coefficient, r [-]</help_brief>
      <default>8.1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/R">
      <label>Submerged specific gravity of gravel</label>
      <help_brief>Submerged specific gravity of gravel</help_brief>
      <default>1.65</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/l">
      <label>bed porosity, gravel</label>
      <help_brief>bed porosity, gravel</help_brief>
      <default>0.4</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/u">
      <label>upwinding coefficient for load spatial derivatives in Exner equation (>0.5 suggested)</label>
      <help_brief>upwinding coefficient for load spatial derivatives in Exner equation (>0.5 suggested)</help_brief>
      <default>.75</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/a">
      <label>coefficient for material transferred to substrate as bed aggrades</label>
      <help_brief>coefficient for material transferred to substrate as bed aggrades</help_brief>
      <default>.5</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/O">
      <label>channel sinuosity</label>
      <help_brief>channel sinuosity</help_brief>
      <default>1.5</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/B">
      <label>ratio of depositional width to channel width</label>
      <help_brief>ratio of depositional width to channel width</help_brief>
      <default>60</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/V">
      <label>ratio of wash load deposited per unit bed material load deposited</label>
      <help_brief>ratio of wash load deposited per unit bed material load deposited</help_brief>
      <default>1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/Input/Var/C">
      <label>Chezy resistance coefficient [-]</label>
      <help_brief>Chezy resistance coefficient [-]</help_brief>
      <default>44</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
  </tab>
  <tab name="About">
    <entry name="/STM/AgDegNormGravMixSubPW/ModelName">
      <label>Model name</label>
      <help_brief>Name of the model</help_brief>
      <default>AgDegNormGravMixSubPW</default>
      <type>String</type>
    </entry>
    <entry name="/STM/AgDegNormGravMixSubPW/ModelAuthor">
      <label>Author name</label>
      <help_brief>Name of the model author</help_brief>
      <default>Gary Parker</default>
      <type>String</type>
    </entry>
  </tab>
</dialog>
