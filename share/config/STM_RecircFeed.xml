<dialog>
  <tab name="Input Files and Directories">
    <entry name="/STM/RecircFeed/Input/Dir">
      <label>Input directory</label>
      <help_brief>Path to input files. {cb;GUI;/data/sims/stm/RecircFeed}</help_brief>
      <default>GUI</default>
      <type>String</type>
    </entry>
    <entry name="/STM/RecircFeed/SitePrefix">
      <label>Site prefix</label>
      <help_brief>Site prefix for input/output files.</help_brief>
      <default>RecircFeed</default>
      <type>String</type>
    </entry>
    <entry name="/STM/RecircFeed/CasePrefix">
      <label>Case prefix</label>
      <help_brief>Case prefix for input/output files.</help_brief>
      <default>test</default>
      <type>String</type>
    </entry>
  </tab>
  <tab name="Run Options">
    <entry name="/STM/RecircFeed/Input/Var/flume">
      <label>Feed-1 or Recirculate-2</label>
      <help_brief>Feed or Recirculate, Feed-1 or Recirculate-2</help_brief>
      <default>1</default>
      <type>Int</type>
      <range>
        <min>1</min>
        <max>2</max>
      </range>
    </entry>
  </tab>
  <tab name="Run Parameters">
    <entry name="/STM/RecircFeed/Input/Var/F">
      <label>Equilibrium Froude number (must be &lt; 1)</label>
      <help_brief>Equilibrium Froude number (must be &lt; 1)</help_brief>
      <default>0.4</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/RecircFeed/Input/Var/n">
      <label>Exponent in load relation</label>
      <help_brief>Exponent in load relation</help_brief>
      <default>1.5</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/RecircFeed/Input/Var/f">
      <label>Backwater number</label>
      <help_brief>Backwater number</help_brief>
      <default>10</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/RecircFeed/Input/Var/e">
      <label>Initial dimensionless mean bed elevation</label>
      <help_brief>Initial dimensionless mean bed elevation</help_brief>
      <default>0</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/RecircFeed/Input/Var/T">
      <label>Ratio of normal to critical Shields stress</label>
      <help_brief>Ratio of normal to critical Shields stress</help_brief>
      <default>1.3</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/RecircFeed/Input/Var/S">
      <label>Initial normalized slope</label>
      <help_brief>Initial normalized slope</help_brief>
      <default>0.5</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/RecircFeed/Input/Var/M">
      <label>Number of spatial intervals desired</label>
      <help_brief>Number of spatial intervals desired</help_brief>
      <default>20</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>10000</max>
      </range>
    </entry>
    <entry name="/STM/RecircFeed/Input/Var/t">
      <label>Dimensionless time step</label>
      <help_brief>Dimensionless time step</help_brief>
      <default>0.0004</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/RecircFeed/Input/Var/p">
      <label>Number of prints desired</label>
      <help_brief>Number of prints desired</help_brief>
      <default>10</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
    <entry name="/STM/RecircFeed/Input/Var/i">
      <label>Number of iterations desired</label>
      <help_brief>Number of iterations desired</help_brief>
      <default>200</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
  </tab>
  <tab name="About">
    <entry name="/STM/RecircFeed/ModelName">
      <label>Model name</label>
      <help_brief>Name of the model</help_brief>
      <default>RecircFeed</default>
      <type>String</type>
    </entry>
    <entry name="/STM/RecircFeed/ModelAuthor">
      <label>Author name</label>
      <help_brief>Name of the model author</help_brief>
      <default>Gary Parker</default>
      <type>String</type>
    </entry>
  </tab>
</dialog>
