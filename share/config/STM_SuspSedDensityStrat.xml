<dialog>
  <tab name="Input Files and Directories">
    <entry name="/STM/SuspSedDensityStrat/Input/Dir">
      <label>Input directory</label>
      <help_brief>Path to input files. {cb;GUI;/data/sims/stm/SuspSedDensityStrat}</help_brief>
      <default>GUI</default>
      <type>String</type>
    </entry>
    <entry name="/STM/SuspSedDensityStrat/SitePrefix">
      <label>Site prefix</label>
      <help_brief>Site prefix for input/output files.</help_brief>
      <default>SuspSedDensityStrat</default>
      <type>String</type>
    </entry>
    <entry name="/STM/SuspSedDensityStrat/CasePrefix">
      <label>Case prefix</label>
      <help_brief>Case prefix for input/output files.</help_brief>
      <default>test</default>
      <type>String</type>
    </entry>
  </tab>
  <tab name="Run Parameters">
    <entry name="/STM/SuspSedDensityStrat/Input/Var/R">
      <label>Specific Gravity of Sediment</label>
      <help_brief>Specific Gravity of Sediment</help_brief>
      <default>2.65</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/SuspSedDensityStrat/Input/Var/D">
      <label>Sediment Grain Size [mm]</label>
      <help_brief>Sediment Grain Size [mm]</help_brief>
      <default>0.1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/SuspSedDensityStrat/Input/Var/H">
      <label>Flow Depth [m]</label>
      <help_brief>Flow Depth [m]</help_brief>
      <default>1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/SuspSedDensityStrat/Input/Var/k">
      <label>Composite Roughness Height (including bedform effects)[mm]</label>
      <help_brief>Composite Roughness Height (including bedform effects)[mm]</help_brief>
      <default>2</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/SuspSedDensityStrat/Input/Var/u">
      <label>Shear Velocity [cm/s]</label>
      <help_brief>Shear Velocity [cm/s]</help_brief>
      <default>2</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/SuspSedDensityStrat/Input/Var/v">
      <label>Kinematic Viscosity of Water [cm^2/s]</label>
      <help_brief>Kinematic Viscosity of Water [cm^2/s]</help_brief>
      <default>0.01</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/SuspSedDensityStrat/Input/Var/C">
      <label>Shear Velocity due to Skin Friction [cm/s] </label>
      <help_brief>Shear Velocity due to Skin Friction [cm/s] </help_brief>
      <default>2.0</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
  </tab>
  <tab name="About">
    <entry name="/STM/SuspSedDensityStrat/ModelName">
      <label>Model name</label>
      <help_brief>Name of the model</help_brief>
      <default>SuspSedDensityStrat</default>
      <type>String</type>
    </entry>
    <entry name="/STM/SuspSedDensityStrat/ModelAuthor">
      <label>Author name</label>
      <help_brief>Name of the model author</help_brief>
      <default>Gary Parker</default>
      <type>String</type>
    </entry>
  </tab>
</dialog>
