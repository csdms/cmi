<dialog>
  <tab name="Input Files and Directories">
    <entry name="/Sedflux/Input/Dir">
      <label>Input directory</label>
      <help_brief>Path to input files. {cb;GUI;/data/sims/sedflux/delta_20x40}</help_brief>
      <default>GUI</default>
      <type>String</type>
    </entry>
    <entry name="/Sedflux/SitePrefix">
      <label>Site prefix</label>
      <help_brief>Site prefix for input/output files.</help_brief>
      <default>small</default>
      <type>String</type>
    </entry>
    <entry name="/Sedflux/CasePrefix">
      <label>Case prefix</label>
      <help_brief>Case prefix for input/output files.</help_brief>
      <default>init</default>
      <type>String</type>
    </entry>
  </tab>
  <tab name="Ports">
    <entry name="/Sedflux/Port/WaterDischarge">
      <label>Water discharge port</label>
      <help_brief>Use the WaterDischarge provides port. {dl;ON;OFF}</help_brief>
      <default>ON</default>
      <type>String</type>
    </entry>
    <entry name="/Sedflux/Port/SubaerialErosion">
      <label>Erosion port</label>
      <help_brief>Use the Erosion provides port. {dl;ON;OFF}</help_brief>
      <default>ON</default>
      <type>String</type>
    </entry>
  </tab>
  <tab name="Run Parameters">
    <entry name="/Sedflux/Input/Var/RunDuration">
      <label>Run duraction (years)</label>
      <help_brief>Simulation run time in years</help_brief>
      <default>500</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/XResolution">
      <label>Grid resolution in x-direction (m)</label>
      <help_brief>Grid resolution in x-direction (m)</help_brief>
      <default>1000</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>10000</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/YResolution">
      <label>Grid resolution in y-direction (m)</label>
      <help_brief>Grid resolution in y-direction (m)</help_brief>
      <default>1000</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>10000</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/ZResolution">
      <label>Grid resolution in z-direction (m)</label>
      <help_brief>grid resolution in z-direction (m)</help_brief>
      <default>.5</default>
      <type>Float</type>
      <range>
        <min>0.01</min>
        <max>10</max>
      </range>
    </entry>
  </tab>
  <tab name="Processes">
    <entry name="/Sedflux/Input/Var/AvulsionStdDev">
      <label>Avulsion standard deviation (degrees)</label>
      <help_brief>Avulsion standard deviation (degrees)</help_brief>
      <default>0</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>90</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/AvulsionMinAngle">
      <label>Minimum river angle [degrees]</label>
      <help_brief>Minimum river angle [degrees]</help_brief>
      <default>-90</default>
      <type>Float</type>
      <range>
        <min>-180</min>
        <max>180</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/AvulsionMaxAngle">
      <label>Maximum river angle [degrees]</label>
      <help_brief>Maximum river angle [degrees]</help_brief>
      <default>-90</default>
      <type>Float</type>
      <range>
        <min>-180</min>
        <max>180</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/AvulsionHingeRow">
      <label>Row position of river hinge point</label>
      <help_brief>Row position of river hinge point</help_brief>
      <default>50</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>1000</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/AvulsionHingeCol">
      <label>Column position of river hinge point</label>
      <help_brief>Column position of river hinge point</help_brief>
      <default>99</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>1000</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/PlumeCurrentVelocity">
      <label>Velocity of coastal current (m/s)</label>
      <help_brief>Velocity of coastal current (m/s)</help_brief>
      <default>0</default>
      <type>Float</type>
      <range>
        <min>-5</min>
        <max>5</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/SuspendedLoadConc">
      <label>Suspended load concentration (-)</label>
      <help_brief>Per-grain suspended load concentration (-)</help_brief>
      <default>.01</default>
      <type>Float</type>
      <range>
        <min>0.01</min>
        <max>5</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/BedLoadDumpDistance">
      <label>Distance to dump bedload (m)</label>
      <help_brief>Distance to dump bedload (m)</help_brief>
      <default>5000</default>
      <type>Float</type>
      <range>
        <min>100</min>
        <max>50000</max>
      </range>
    </entry>
    <entry name="/Sedflux/Input/Var/BedLoadFlux">
      <label>Bed load flux (kg/s)</label>
      <help_brief>Bed load flux at river mouth (kg/s)</help_brief>
      <default>1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>5000</max>
      </range>
    </entry>
  </tab>
  <tab name="Output Grids">
    <entry name="/Sedflux/Output/Grid/Dir">
      <label>Output directory</label>
      <help_brief>Path to output grid files.</help_brief>
      <default>${cwd}</default>
      <type>String</type>
    </entry>
    <entry name="/Sedflux/Output/Grid/Interval">
      <label>Interval between output files</label>
      <help_brief>Interval between output files</help_brief>
      <default>10</default>
      <range>
        <min>0</min>
        <max>1000000</max>
      </range>
      <type>Float</type>
    </entry>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorDepth"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorSlope"/>
    <entry class="OutputFile"
           name="/Sedflux/Output/Grid/Var/SeaFloorElevation"/>
    <entry class="OutputFile"
           name="/Sedflux/Output/Grid/Var/SeaFloorThickness"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorGrain"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorAge"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorSand"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorSilt"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorClay"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorMud"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorFacies"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorDensity"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorPorosity"/>
    <entry class="OutputFile"
           name="/Sedflux/Output/Grid/Var/SeaFloorPermeability"/>
    <entry class="OutputFile" name="/Sedflux/Output/Grid/Var/SeaFloorBasement"/>
    <entry class="OutputFile"
           name="/Sedflux/Output/Grid/Var/SeaFloorRiver_mouth"/>
  </tab>
  <tab name="Output Cubes">
    <entry name="/Sedflux/Output/Cube/Dir">
      <label>Output directory</label>
      <help_brief>Path to output cube files.</help_brief>
      <default>${cwd}</default>
      <type>String</type>
    </entry>
    <entry name="/Sedflux/Output/Cube/Interval">
      <label>Interval between output files</label>
      <help_brief>Interval between output files</help_brief>
      <default>10</default>
      <range>
        <min>0</min>
        <max>1000000</max>
      </range>
      <type>Int</type>
    </entry>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Age"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Facies"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Pressure"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Density"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Grain_density"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Max_density"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Grain"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Grain_in_meters"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Sand"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Silt"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Clay"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Mud"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Velocity"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Viscosity"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Relative_density"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Porosity"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Porosity_min"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Porosity_max"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Pi"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Permeability"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Void_ratio"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Void_ratio_min"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Void_ratio_max"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Friction_angle"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Consolidation"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Yield_strength"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Dynamic_viscosity"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Mv"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Cv"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Hydraulic_con"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Shear_strength"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Cohesion"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Consolidation"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Consolidation_rate"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Excess_pressure"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Relative_pressure"/>
    <entry class="OutputFile" name="/Sedflux/Output/Cube/Var/Fraction"/>
  </tab>
  <tab name="About">
    <entry name="/Sedflux/ModelName">
      <label>Model name</label>
      <help_brief>Name of the model</help_brief>
      <default>sedflux</default>
      <type>String</type>
    </entry>
    <entry name="/Sedflux/ModelAuthor">
      <label>Author name</label>
      <help_brief>Name of the model author</help_brief>
      <default>Eric Hutton</default>
      <type>String</type>
    </entry>
  </tab>
</dialog>
