<dialog>
  <tab name="Input Files and Directories">
    <entry name="/STM/RouseVanoniEquilibrium/Input/Dir">
      <label>Input directory</label>
      <help_brief>Path to input files. {cb;GUI;/data/sims/stm/RouseVanoniEquilibrium}</help_brief>
      <default>GUI</default>
      <type>String</type>
    </entry>
    <entry name="/STM/RouseVanoniEquilibrium/SitePrefix">
      <label>Site prefix</label>
      <help_brief>Site prefix for input/output files.</help_brief>
      <default>RouseVanoniEquilibrium</default>
      <type>String</type>
    </entry>
    <entry name="/STM/RouseVanoniEquilibrium/CasePrefix">
      <label>Case prefix</label>
      <help_brief>Case prefix for input/output files.</help_brief>
      <default>test</default>
      <type>String</type>
    </entry>
  </tab>
  <tab name="Run Parameters">
    <entry name="/STM/RouseVanoniEquilibrium/Input/Var/b">
      <label>non-dimensional distance from the bed</label>
      <help_brief>c=cb</help_brief>
      <default>0.05</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/RouseVanoniEquilibrium/Input/Var/v">
      <label>vs settling velocity [cm/s]</label>
      <help_brief>vs settling velocity [cm/s]</help_brief>
      <default>3</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/RouseVanoniEquilibrium/Input/Var/u">
      <label>u* shear velocity [m/s]</label>
      <help_brief>u* shear velocity [m/s]</help_brief>
      <default>0.2</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
  </tab>
  <tab name="About">
    <entry name="/STM/RouseVanoniEquilibrium/ModelName">
      <label>Model name</label>
      <help_brief>Name of the model</help_brief>
      <default>RouseVanoniEquilibrium</default>
      <type>String</type>
    </entry>
    <entry name="/STM/RouseVanoniEquilibrium/ModelAuthor">
      <label>Author name</label>
      <help_brief>Name of the model author</help_brief>
      <default>Gary Parker</default>
      <type>String</type>
    </entry>
  </tab>
</dialog>
