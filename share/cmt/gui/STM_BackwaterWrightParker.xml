<dialog>
  <tab name="Input Files and Directories">
    <entry name="/STM/BackwaterWrightParker/Input/Dir">
      <label>Input directory</label>
      <help_brief>Path to input files. {cb;GUI;/data/sims/stm/BackwaterWrightParker}</help_brief>
      <default>GUI</default>
      <type>String</type>
    </entry>
    <entry name="/STM/BackwaterWrightParker/SitePrefix">
      <label>Site prefix</label>
      <help_brief>Site prefix for input/output files.</help_brief>
      <default>BackwaterWrightParker</default>
      <type>String</type>
    </entry>
    <entry name="/STM/BackwaterWrightParker/CasePrefix">
      <label>Case prefix</label>
      <help_brief>Case prefix for input/output files.</help_brief>
      <default>test</default>
      <type>String</type>
    </entry>
  </tab>
  <tab name="Run Parameters">
    <entry name="/STM/BackwaterWrightParker/Input/Var/S">
      <label>bed slope</label>
      <help_brief>bed slope</help_brief>
      <default>0.0001</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/BackwaterWrightParker/Input/Var/R">
      <label>Submerged specific gravity of sediment</label>
      <help_brief>Submerged specific gravity of sediment</help_brief>
      <default>1.65</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/BackwaterWrightParker/Input/Var/d">
      <label>median grain size (D50) [mm]</label>
      <help_brief>median grain size (D50) [mm]</help_brief>
      <default>0.35</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/BackwaterWrightParker/Input/Var/D">
      <label>grain size such that 90% passes (D90) [mm]</label>
      <help_brief>grain size such that 90% passes (D90) [mm]</help_brief>
      <default>1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/BackwaterWrightParker/Input/Var/B">
      <label>channel width [m]</label>
      <help_brief>channel width [m]</help_brief>
      <default>275</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/BackwaterWrightParker/Input/Var/Q">
      <label>flow discharge [m^3/s]</label>
      <help_brief>flow discharge [m^3/s]</help_brief>
      <default>1800</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/BackwaterWrightParker/Input/Var/k">
      <label>downstream water surface elevation [m]</label>
      <help_brief>downstream water surface elevation [m]</help_brief>
      <default>20</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/BackwaterWrightParker/Input/Var/L">
      <label>reach length [m]</label>
      <help_brief>reach length [m]</help_brief>
      <default>200000</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/BackwaterWrightParker/Input/Var/M">
      <label>number of spatial nodes (max of 99)</label>
      <help_brief>number of spatial nodes (max of 99)</help_brief>
      <default>50</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
  </tab>
  <tab name="About">
    <entry name="/STM/BackwaterWrightParker/ModelName">
      <label>Model name</label>
      <help_brief>Name of the model</help_brief>
      <default>BackwaterWrightParker</default>
      <type>String</type>
    </entry>
    <entry name="/STM/BackwaterWrightParker/ModelAuthor">
      <label>Author name</label>
      <help_brief>Name of the model author</help_brief>
      <default>Gary Parker</default>
      <type>String</type>
    </entry>
  </tab>
</dialog>
