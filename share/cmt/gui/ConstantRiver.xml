<dialog>
  <tab name="River Parameters">
    <entry name="/ConstantRiver/Input/BedLoadFlux">
      <label>Bed load flux</label>
      <unit>kg/s</unit>
      <help_brief>Bed load flux [kg/s]</help_brief>
      <default>250</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e37</max>
      </range>
    </entry>
    <entry name="/ConstantRiver/Input/Concentration">
      <label>Concentration</label>
      <unit>-</unit>
      <help_brief>Concentration of suspended load [-]</help_brief>
      <default>.1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e37</max>
      </range>
    </entry>
    <entry name="/ConstantRiver/Input/Velocity">
      <label>Velocity</label>
      <unit>m/s</unit>
      <help_brief>River velocity [m/s]</help_brief>
      <default>2</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e37</max>
      </range>
    </entry>
    <entry name="/ConstantRiver/Input/Width">
      <label>Width</label>
      <unit>m</unit>
      <help_brief>River width [m]</help_brief>
      <default>200</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e37</max>
      </range>
    </entry>
    <entry name="/ConstantRiver/Input/Depth">
      <label>Depth</label>
      <unit>m</unit>
      <help_brief>River depth [m]</help_brief>
      <default>10</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e37</max>
      </range>
    </entry>
  </tab>
  <tab name="About">
    <entry name="/ConstantRiver/ModelName">
      <label>Model name</label>
      <help_brief>Name of the model</help_brief>
      <default>ConstantRiver</default>
      <type>String</type>
    </entry>
    <entry name="/ConstantRiver/ModelAuthor">
      <label>Author name</label>
      <help_brief>Name of the model author</help_brief>
      <default>Eric Hutton</default>
      <type>String</type>
    </entry>
  </tab>
</dialog>
