<dialog>
<!-- ====================================================================== -->
  <tab name="Input 1">
    <entry name="/ErodeGlobal/Input/Var/comp_status">
      <label>Component status:</label>
      <default>Enabled</default>
      <type>string</type>
      <help_brief>component status {Enabled; Disabled}</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/in_directory">
      <label>Input directory:</label>
      <default>~/CMT_Output</default>
      <type>string</type>
      <help_brief>input directory</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/out_directory">
      <label>Output directory:</label>
      <default>~/CMT_Output/</default>
      <type>string</type>
      <help_brief>output directory</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/site_prefix">
      <label>Site prefix:</label>
      <default>Test</default>
      <type>string</type>
      <help_brief>prefix for the study site</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/case_prefix">
      <label>Case prefix:</label>
      <default>Test</default>
      <type>string</type>
      <help_brief>prefix for a given scenario</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/stop_code">
      <label>Stop method code:</label>
      <default>0</default>
      <type>long</type>
      <range>
        <min>0</min>
        <max>2</max>
      </range>
      <help_brief>stop method code: 0=n_steps, 1=stop_time, 2=steady-state</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/n_steps">
      <label>Number of steps:</label>
      <default>1000</default>
      <type>long</type>
      <range>
        <min>1</min>
        <max>1e9</max>
      </range>
      <help_brief>number of time steps, if stop_code=0</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/stop_time">
      <label>Time to stop:</label>
      <default>1000.0</default>
      <type>float</type>
      <range>
        <min>1</min>
        <max>1e8</max>
      </range>
      <help_brief>time to stop [years], if stop_code=1</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/nx">
      <label>Grid nx:</label>
      <default>100</default>
      <type>long</type>
      <range>
        <min>10</min>
        <max>2000</max>
      </range>
      <help_brief>number of cols in DEM</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/ny">
      <label>Grid ny:</label>
      <default>100</default>
      <type>long</type>
      <range>
        <min>10</min>
        <max>2000</max>
      </range>
      <help_brief>number of rows in DEM</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/dx">
      <label>Grid cell dx:</label>
      <default>50.0</default>
      <type>float</type>
      <range>
        <min>1.0</min>
        <max>1000.0</max>
      </range>
      <help_brief>grid cell xsize [meters]</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/dy">
      <label>Grid cell dy:</label>
      <default>50.0</default>
      <type>float</type>
      <range>
        <min>1.0</min>
        <max>1000.0</max>
      </range>
      <help_brief>grid cell ysize [meters]</help_brief>
    </entry>

  </tab>

<!-- ====================================================================== -->
  <tab name="Input 2">
    <entry name="/ErodeGlobal/Input/Var/CFL_factor">
      <label>CFL factor:</label>
      <default>0.2</default>
      <type>float</type>
      <range>
        <min>0.0</min>
        <max>1.0</max>
      </range>
      <help_brief>prefactor for CFL stability condition</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/m">
      <label>Area exponent, m:</label>
      <default>1.5</default>
      <type>float</type>
      <range>
        <min>1.0</min>
        <max>2.0</max>
      </range>
      <help_brief>area exponent</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/n">
      <label>Slope exponent, n:</label>
      <default>1.0</default>
      <type>float</type>
      <range>
        <min>1.0</min>
        <max>2.0</max>
      </range>
      <help_brief>slope exponent</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/K">
      <label>Erodibility, K:</label>
      <default>1e-2</default>
      <type>float</type>
      <range>
        <min>1e-5</min>
        <max>1e-1</max>
      </range>
      <help_brief>erodibility</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/p">
      <label>Q-A exponent, p:</label>
      <default>1.0</default>
      <type>float</type>
      <range>
        <min>0.1</min>
        <max>1.0</max>
      </range>
      <help_brief>Q-A exponent</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/R">
      <label>Mean rainrate, R:</label>
      <default>1.0</default>
      <type>float</type>
      <range>
        <min>0.0</min>
        <max>5.0</max>
      </range>
      <help_brief>geomorphically effective rainrate [m/yr]</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/U">
      <label>Mean uplift rate, U:</label>
      <default>0.0</default>
      <type>float</type>
      <range>
        <min>0.0</min>
        <max>2.0</max>
      </range>
      <help_brief>uplift rate [mm/yr]</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/BLR">
      <label>Base lowering rate:</label>
      <default>0.1</default>
      <type>float</type>
      <range>
        <min>0.0</min>
        <max>2.0</max>
      </range>
      <help_brief>base-level lowering rate [mm/yr]</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/BC_method">
      <label>BC method:</label>
      <default>FOUR_SIDES</default>
      <type>string</type>
      <help_brief>boundary-condition method {BOTTOM; RIGHT; FOUR_SIDES}</help_brief>
    </entry>

  </tab>

<!-- ====================================================================== -->
  <tab name="Z0 Options">
    <entry name="/ErodeGlobal/Input/Var/make_z0_method">
      <label>Make z0 method:</label>
      <default>FLAT</default>
      <type>string</type>
      <help_brief>method to make initial DEM {FLAT; PLANE; CORNER_PLANE; READ_FILE}</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/z0_plane_dz_dx">
      <label>z0 plane dz/dx:</label>
      <default>0.1</default>
      <type>float</type>
      <range>
        <min>0.0</min>
        <max>10.0</max>
      </range>
      <help_brief>dz/dx for make_z0_method=PLANE</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/z0_plane_dz_dy">
      <label>z0 plane dz/dy:</label>
      <default>0.0</default>
      <type>float</type>
      <range>
        <min>0.0</min>
        <max>10.0</max>
      </range>
      <help_brief>dz/dy for make_z0_method=PLANE</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/z0_file">
      <label>z0 filename:</label>
      <default>[site_prefix]_2D-z0.rtg</default>
      <type>string</type>
      <help_brief>filename for make_z0_method=READ_FILE</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/noise_method">
      <label>Noise method:</label>
      <default>MIDPOINTS</default>
      <type>string</type>
      <help_brief>noise method {MIDPOINTS; GAUSSIAN; O_NOISE}</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/noise_scale">
      <label>Noise scale:</label>
      <default>1.0</default>
      <type>float</type>
      <range>
        <min>0.1</min>
        <max>1000.0</max>
      </range>
      <help_brief>scale factor for random noise</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/seed">
      <label>Random seed:</label>
      <default>36421</default>
      <type>long</type>
      <range>
        <min>10000</min>
        <max>99999</max>
      </range>
      <help_brief>random number seed (long int)</help_brief>
    </entry>

  </tab>

<!-- ====================================================================== -->
  <tab name="Output 1">
    <entry name="/ErodeGlobal/Input/Var/save_grid_dt">
      <label>Save grid dt:</label>
      <default>100.0</default>
      <type>float</type>
      <range>
        <min>1.0</min>
        <max>1e5</max>
      </range>
      <help_brief>time interval between saved grids [years]</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/SAVE_Z_GRIDS">
      <label>Save z grids toggle:</label>
      <default>Yes</default>
      <type>string</type>
      <help_brief>option to save computed z grids {Yes; No}</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/z_gs_file">
