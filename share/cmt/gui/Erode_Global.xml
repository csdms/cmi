      <range>
        <min>1.0</min>
        <max>1e5</max>
      </range>
      <help_brief>time interval between time series values [years]</help_brief>
    </entry>

    <entry name="/ErodeGlobal/Input/Var/pixel_file">
      <label>Monitored pixels file:</label>
      <default>[case_prefix]_outlets.txt</default>
