<dialog>
  <tab name="Input Files and Directories">
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Dir">
      <label>Input directory</label>
      <help_brief>Path to input files. {cb;GUI;/data/sims/stm/AgDegNormalGravMixHyd}</help_brief>
      <default>/data/sims/stm/AgDegNormalGravMixHyd</default>
      <type>String</type>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/SitePrefix">
      <label>Site prefix</label>
      <help_brief>Site prefix for input/output files.</help_brief>
      <default>AgDegNormalGravMixHyd</default>
      <type>String</type>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/CasePrefix">
      <label>Case prefix</label>
      <help_brief>Case prefix for input/output files.</help_brief>
      <default>test</default>
      <type>String</type>
    </entry>
  </tab>
  <tab name="Run Parameters">
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/N">
      <label>number of hydrograph entries [between 2 and 16 values]</label>
      <help_brief>number of hydrograph entries [between 2 and 16 values]</help_brief>
      <default>10</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/c">
      <label>number of cycles per year</label>
      <help_brief>number of cycles per year</help_brief>
      <default>1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/e">
      <label>bed elevation at downstream end [m]</label>
      <help_brief>bed elevation at downstream end [m]</help_brief>
      <default>0</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/S">
      <label>initial bed slope</label>
      <help_brief>initial bed slope</help_brief>
      <default>0.00262</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/L">
      <label>reach length [m]</label>
      <help_brief>reach length [m]</help_brief>
      <default>10000</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/t">
      <label>Time step [days]</label>
      <help_brief>Time step [days]</help_brief>
      <default>1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/M">
      <label>Number of intervals</label>
      <help_brief>Number of intervals</help_brief>
      <default>10</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/p">
      <label>Number of printouts</label>
      <help_brief>Number of printouts</help_brief>
      <default>6</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/i">
      <label>Iterations per each printout</label>
      <help_brief>Iterations per each printout</help_brief>
      <default>200</default>
      <type>Int</type>
      <range>
        <min>0</min>
        <max>100000</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/k">
      <label>factor by which Ds90 is multiplied for roughness height</label>
      <help_brief>factor by which Ds90 is multiplied for roughness height</help_brief>
      <default>2</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/n">
      <label>factor by which Ds90 is multiplied for active layer thickness</label>
      <help_brief>factor by which Ds90 is multiplied for active layer thickness</help_brief>
      <default>2</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/r">
      <label>Manning-Strickler coefficient, r [-]</label>
      <help_brief>Manning-Strickler coefficient, r [-]</help_brief>
      <default>8.1</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/R">
      <label>Submerged specific gravity of sediment</label>
      <help_brief>Submerged specific gravity of sediment</help_brief>
      <default>1.65</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/l">
      <label>bed porosity, gravel</label>
      <help_brief>bed porosity, gravel</help_brief>
      <default>0.4</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/u">
      <label>upwinding coefficient for load spatial derivatives in Exner equation (>0.5 suggested)</label>
      <help_brief>upwinding coefficient for load spatial derivatives in Exner equation (>0.5 suggested)</help_brief>
      <default>0.75</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/Input/Var/a">
      <label>coefficient for material transferred to substrate as bed aggrades</label>
      <help_brief>coefficient for material transferred to substrate as bed aggrades</help_brief>
      <default>0.5</default>
      <type>Float</type>
      <range>
        <min>0</min>
        <max>1e32</max>
      </range>
    </entry>
  </tab>
  <tab name="About">
    <entry name="/STM/AgDegNormalGravMixHyd/ModelName">
      <label>Model name</label>
      <help_brief>Name of the model</help_brief>
      <default>AgDegNormalGravMixHyd</default>
      <type>String</type>
    </entry>
    <entry name="/STM/AgDegNormalGravMixHyd/ModelAuthor">
      <label>Author name</label>
      <help_brief>Name of the model author</help_brief>
      <default>Gary Parker</default>
      <type>String</type>
    </entry>
  </tab>
</dialog>
